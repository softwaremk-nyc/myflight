{
  "name": "myflight",
  "version": "1.0.0",
  "description": "basic flight performance calculation",
  "main": "dist/index.js",
  "scripts": {
    "build": "eslint . --ext .js,.ts && tsc",
    "start:dev": "concurrently --kill-others --names \"LINT,TSC,TEST\" npm:watch-lint npm:watch-tsc npm:watch-test",
    "test": "npm run build && jest",
    "watch-lint": "esw . --ext .js,.ts -w",
    "watch-tsc": "tsc -w",
    "watch-test": "jest --watchAll",
    "start:server": "nodemon dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://softwaremk@bitbucket.org/softwaremk/myflight.git"
  },
  "author": "softwaremk@outlook.com",
  "license": "ISC",
  "homepage": "https://bitbucket.org/softwaremk/myflight#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.9.7",
    "apollo-server": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "express": "^4.17.1"
  }
}
