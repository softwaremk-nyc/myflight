{
  "name": "myflight",
  "version": "1.0.0",
  "description": "basic flight performance calculation",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "node dist/server/index.js",
    "build": "npm run clean && tsc -p ./server && webpack --mode production",
    "build:dev": "npm run clean && eslint . --ext .js,.ts && tsc",
    "start:dev": "npm run build:dev && concurrently --kill-others --names \"LINT,TSC,TEST\" npm:watch-lint npm:watch-tsc npm:watch-test",
    "start:server": "nodemon dist/server/index.js",
    "start:client": "webpack serve --mode=development --content-base ./dist/client --hot --inline",
    "test": "npm run build:dev && jest",
    "watch-lint": "esw . --ext .js,.ts -w",
    "watch-tsc": "tsc -w",
    "watch-test": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://softwaremk@bitbucket.org/softwaremk/myflight.git"
  },
  "author": "softwaremk@outlook.com",
  "license": "ISC",
  "homepage": "https://bitbucket.org/softwaremk/myflight#readme",
  "devDependencies": {
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@reduxjs/toolkit": "^1.5.0",
    "apollo-datasource-rest": "^0.9.7",
    "apollo-server": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "chart.js": "^2.9.4",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-debounce-input": "^3.2.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2"
  }
}
